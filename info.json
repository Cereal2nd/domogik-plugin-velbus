    "commands": {
       "set_level_bin": {
           "name": "Switch On or Off",
           "return_confirmation": true,
           "parameters": [
               {
                   "key": "level",
                   "data_type": "DT_Switch",
                   "conversion": "from_DT_Switch_to_level"
               }
           ],
           "xpl_command": "set_level_bin"
        },
        "set_level_range": {
           "name": "Set to a level",
           "return_confirmation": true,
           "parameters": [
               {
                   "key": "level",
                   "data_type": "DT_Scale",
                   "conversion": "from_DT_Scale_to_level"
               }
           ],
           "xpl_command": "set_level_range"
        }
    },
