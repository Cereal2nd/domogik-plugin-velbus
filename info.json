{
    "configuration": [
        {
            "default": "0", 
            "description": "define the connection type to velbus", 
            "key": "connection-type", 
            "name": "Connection type",
            "required": "yes", 
            "choices": [
                "serial", 
                "socket"
            ], 
            "type": "choice"
        }, 
        {
            "default": "/dev/ttyACM0", 
            "description": "velbus device (/dev/ttyACM0 for serial, or ip:port for socket", 
            "key": "device", 
            "name": "Device",
            "required": "yes", 
            "type": "string"
        }
    ], 
    "xpl_commands": {
         "set_level_bin": {
            "name": "blah",
            "schema": "lighting.basic",
            "xplstat_name": "get_level_bin",
            "parameters": {
                "static": [],
                "device": []
            }
         },
         "set_level_range": {
            "name": "set_level_range",
            "schema": "lighting.basic",
            "xplstat_name": "get_level_range",
            "parameters": {
                "static": [],
                "device": []
            }
         }
    },
    "xpl_stats": {
       "get_level_bin": {
            "name": "get_level_bin",
            "schema": "lighting.device",
            "parameters": {
                "static": [],
                "device": [],
                "dynamic": [
                    {
                         "key": "level",
                         "ignore_values": "",
                         "sensor": "level_bin"
                    }
                ]
            }
       },
       "get_level_range": {
            "name": "get_level_range",
