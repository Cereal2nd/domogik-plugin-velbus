    "configuration": [
        {
            "default": "False", 
            "description": "Automatically start plugin at Domogik startup", 
            "key": "startup-plugin", 
            "name": "startup plugin",
            "required": "yes", 
            "type": "boolean"
        }, 
        {
            "default": "0", 
            "description": "define the connection type to velbus", 
            "key": "connection-type", 
