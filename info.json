            "history": {
                "store": true,
                "duplicate": true,
                "max": 0,
                "expire": 0,
                "round_value": 0
            }
	    }
    },
    "device_types": {
        "velbus.relay": {
            "id": "velbus.relay",
            "description": "Switch one channel on a device", 
            "name": "Switch",
            "commands": ["set_level_bin"],
            "sensors": ["level_bin"],
            "parameters": [
                {
                    "key": "channel",
                    "xpl": true,
                    "description": "The channel number",
                    "type": "integer",
                    "max_value": 4,
                    "min_value": 1
                },
                {
                    "key": "device",
                    "xpl": true,
                    "description": "The decimal address",
                    "type": "integer",
                    "max_value": 255,
                    "min_value": 0
                }
            ]
        }, 
        "velbus.dimmer": {
            "id": "velbus.dimmer",
